{
  "expo": {
    "name": "niuzie",
    "slug": "niuzie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/niuzie-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#02030C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/niuzie-logo.png",
        "backgroundColor": "#02030C"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
