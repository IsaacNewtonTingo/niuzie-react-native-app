{
  "expo": {
    "name": "niuzie",
    "slug": "niuzie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/niuzie-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#02030C",
      "width": 400,
      "height": 400
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "lyft"
        ]
      },
      "bundleIdentifier": "com.niuzie.app",
      "supportsTablet": true,
      "associatedDomains": [
        "applinks:niuzie.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/foreground.png",
        "backgroundColor": "#02030C"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.niuzie.com",
              "pathPrefix": "/product"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.niuzie.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7384df5c-b9fa-4e11-9ebe-f8477a291a32"
      }
    }
  }
}
